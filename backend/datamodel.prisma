type Stock {
  id: ID! @id
  name: String! @unique
  symbol: String! @unique
  opening: Float
  price: Float
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type User {
  id: ID! @id
  name: String!
  email: String!
  money: Float @default(value: 10000000)
  stocks: [StockOrder]
}

type RandomProfile {
  id: ID! @id
  name: String!
  money: Float @default(value: 10000000)
  stocks: [RandomProfileStockOrder]
}

type StockOrder {
  id: ID! @id
  stock: Stock!
  user: User!
  quantity: Float! @default(value: 1)
  purchasedAt: Float!
}

type RandomProfileStockOrder {
  id: ID! @id
  stock: Stock!
  profile: RandomProfile!
  quantity: Float! @default(value: 1)
  purchasedAt: Float!
}