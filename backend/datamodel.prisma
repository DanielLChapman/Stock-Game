type Stock {
  id: ID! @id
  name: String! @unique
  symbol: String! @unique
  opening: Float
  current: Float
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type User {
  id: ID! @id
  name: String!
  email: String!
  money: Float @default(value: 10000000)
  stock: [Stock]
}

type RandomProfile {
  id: ID! @id
  name: String!
  money: Float @default(value: 10000000)
  stock: [Stock]
}